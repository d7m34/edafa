{
  "manifest_version": 3,
  "name": "Security_Lab_POC",
  "version": "1.0",
  "permissions": ["storage", "webRequest", "webRequestAuthProvider"],
  "host_permissions": ["*://*.discord.com/*", "https://*.discordapp.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.discord.com/*"],
      "js": ["content.js"],
      "all_frames": true
    }
  ]
}
