{
  "manifest_version": 3,
  "name": "Ultimate Security Lab POC",
  "version": "4.0",
  "permissions": ["storage", "webNavigation"],
  "host_permissions": ["*://*.discord.com/*", "https://*.discordapp.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.discord.com/*"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
